{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 11
    },
    "env": {
        "browser":true,
        "node":true,
        "es2020": true
    },
    "rules": {
        "semi": ["error", "always"],
        "indent": ["error", 4, { "ignoreComments": true }]
    }
}
